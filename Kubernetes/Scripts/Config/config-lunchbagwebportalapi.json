{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "CallbackPath": "/signin-oidc"
  },
  "CosmosDb": {
    "Endpoint": "https://lunchbagcosmosdb.documents.azure.com:443/",
    "Database": "LunchBag",
    "Collections": {
      "Events": "Events",
      "Notes": "Notes",
      "NoteTemplates": "NoteTemplates",
      "Deliveries": "Deliveries",
      "DeliveryRoutes": "DeliveryRoutes"
    }
  },
  "WebPortalApi": {
    "UseCache": true,
    "MaxNoteItems": 10,
    "MaxSentimentItems": 12
  },
  "MobiliyaApiConfig": {
    "IdentityService": "https://lbfleet-identity-service.azurewebsites.net/",
    "FleetService": "https://lbfleet-fleet-service.azurewebsites.net/",
    "TripService": "https://lbfleet-trip-service.azurewebsites.net/"
  },
  "BingApiConfig": {
    "Url": "https://dev.virtualearth.net/REST/v1/"
  }
}